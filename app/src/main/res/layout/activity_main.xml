    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ugc_banner_close" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ugc_addphoto_camera" />
