    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ugc_needreview_empty_image" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
